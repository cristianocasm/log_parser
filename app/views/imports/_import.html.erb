<div id="<%= dom_id import %>">
  <p>
    <strong>Log file:</strong>
    <%= link_to import.log_file.filename, import.log_file if import.log_file.attached? %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= import.status %>
  </p>

  <p>
    <% import.matches.each do |match| %>
      <% match.cache_report.each do |cache| %>
        <%= cache.stats %>
      <% end %>
    <% end %>
  </p>

</div>
